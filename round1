function fizz(number){
    
    var sum=0;
    while(number){
        sum += parseInt(number%10);
        number = parseInt(number/10);
    }
    var ans =""
    for(var i=1;i<=sum;i++){
        if(i%3==0 && i%4==0)ans += "FizzBuzz ";
        
        else if(i%3==0)ans += "Fizz ";
        
        else if(i%4==0)ans += "Buzz ";
        
        else ans += `${i}`+" ";
    }
    console.log(ans);
    
}
fizz(7009035854)
