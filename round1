
function fizz(num){
    // console.log(num);
  var number=0;
  while(num){
    number = number + (((num % 10) + 10) % 10);
    num/=10;
  }
  console.log(number);
 for(var i=1;i<=number;i++){
      if (i % 3 == 0 && i % 4==0) console.log("FizzBuzz\r");
      else if (i % 3 == 0) console.log("Fizz\r");
      else if (i % 4 == 0) console.log("Buzz\r");
      else console.log(i+"\r")
  }
}
fizz(7009035854);
